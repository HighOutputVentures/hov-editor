{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "import/no-default-export": ["error"],
    "quotes": ["error", "single"]
  }
}
